    "graphlib": "^2.1.8",
    "https-proxy-agent": "^5.0.0",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "js-yaml": "^3.14.1",
    "memoizee": "^0.4.14",
    "minimist": "^1.2.5",
    "open": "^7.3.1",
    "prettyoutput": "^1.2.0",
    "ramda": "^0.27.1",
    "semver": "^7.3.4",
